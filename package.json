{
  "name": "learnteach",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node index",
    "dev": "npx nodemon index",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:rollback": "npx sequelize-cli db:migrate:undo:all",
    "db:refresh": "npm run db:rollback && npm run db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:clean": "npx sequelize-cli db:seed:undo:all",
    "db:seed:refresh": "npm run db:clean && npm run db:seed",
    "db:init": "npm run db:refresh && npm run db:seed"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "http-errors": "~1.6.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.2",
    "sequelize": "^6.33.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1"
  }
}
